# Required: get from ZEGOCLOUD console
VITE_ZEGO_APP_ID=

# Optional: default Zego WebRTC WS API
VITE_ZEGO_WS_SERVER_URL=wss://webrtc-api.zego.im/ws

# Your token issuing endpoint (should not expose ServerSecret to client)
# Example: http://localhost:3001/api/zego/token
VITE_TOKEN_SERVER_URL=

# Agent translated stream ID suffix (server publishes `${userID}${suffix}`)
VITE_AGENT_STREAM_SUFFIX=_translated

